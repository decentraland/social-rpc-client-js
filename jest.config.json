{
  "moduleFileExtensions": ["ts", "js"],
  "transform": {
    "^.+\\.(ts|tsx)$": "ts-jest"
  },
  "coverageDirectory": "coverage",
  "collectCoverageFrom": ["src/**/*.ts"],
  "coveragePathIgnorePatterns": ["/node_modules/", "src/protobuff-types"],
  "testMatch": ["**/tests/*.spec.(ts)"],
  "testEnvironment": "node",
  "resetMocks": true
}
